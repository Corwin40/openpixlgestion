{% extends 'base.html.twig' %}

{% block title %}Intervention{% endblock %}

{% block body %}
    <h1>Intervention</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ intervention.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ intervention.name }}</td>
            </tr>
            <tr>
                <th>Desctiption</th>
                <td>{{ intervention.desctiption }}</td>
            </tr>
            <tr>
                <th>StartedAt</th>
                <td>{{ intervention.startedAt ? intervention.startedAt|date('H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>FinishedAt</th>
                <td>{{ intervention.finishedAt ? intervention.finishedAt|date('H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ intervention.createdAt ? intervention.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ intervention.updatedAt ? intervention.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_admin_intervention_index') }}">back to list</a>

    <a href="{{ path('app_admin_intervention_edit', {'id': intervention.id}) }}">edit</a>

    {{ include('admin/intervention/_delete_form.html.twig') }}
{% endblock %}
