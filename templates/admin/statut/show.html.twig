{% extends 'base.html.twig' %}

{% block title %}Statut{% endblock %}

{% block body %}
    <h1>Statut</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ statut.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ statut.name }}</td>
            </tr>
            <tr>
                <th>Notes</th>
                <td>{{ statut.notes }}</td>
            </tr>
            <tr>
                <th>Price</th>
                <td>{{ statut.price }}</td>
            </tr>
            <tr>
                <th>Hours</th>
                <td>{{ statut.hours }}</td>
            </tr>
            <tr>
                <th>Author</th>
                <td>{{ statut.author }}</td>
            </tr>
            <tr>
                <th>StartedAt</th>
                <td>{{ statut.startedAt }}</td>
            </tr>
            <tr>
                <th>FinishedAt</th>
                <td>{{ statut.finishedAt }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ statut.createdAt ? statut.createdAt|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ statut.updatedAt ? statut.updatedAt|date('Y-m-d') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_admin_statut_index') }}">back to list</a>

    <a href="{{ path('app_admin_statut_edit', {'id': statut.id}) }}">edit</a>

    {{ include('admin/statut/_delete_form.html.twig') }}
{% endblock %}
